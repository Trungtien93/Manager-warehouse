@model MNBEMART.Models.Warehouse
@{
    ViewData["Title"] = "Chi tiết kho";
}
<h2>@ViewData["Title"]</h2>

<dl class="row">
    <dt class="col-sm-2">Tên kho</dt>
    <dd class="col-sm-10">@Model.Name</dd>

    <dt class="col-sm-2">Địa chỉ</dt>
    <dd class="col-sm-10">@Model.Address</dd>
</dl>

<a class="btn btn-warning" asp-action="Edit" asp-route-id="@Model.Id">Sửa</a>
<a class="btn btn-secondary" asp-action="Index">Về danh sách</a>
